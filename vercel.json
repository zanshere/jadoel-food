{
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/(assets|uploads|public|css|js|images)/(.*)", "dest": "/$1/$2" },
    { "src": "/(.*)", "dest": "/index.php" }
  ],
  "build": {
    "env": {
      "VERCEL_PHP_BUILD_RUNTIME": "vercel-php@0.7.3"
    }
  },
  "projectSettings": {
    "framework": "other"
  },
  "functionsDirectory": "."
}
